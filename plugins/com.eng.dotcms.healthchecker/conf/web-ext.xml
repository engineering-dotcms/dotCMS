<servlet>
	<description>Servlet che cambia il receiver per jGroups in modo da controllare eventi di suspect e viewAccept</description>
	<servlet-name>change-receiver-jgroups</servlet-name>
	<servlet-class>com.eng.dotcms.healthchecker.servlet.HealthServlet</servlet-class>
	<load-on-startup>11</load-on-startup>
</servlet>

<listener>
	<listener-class>com.eng.dotcms.healthchecker.listener.InitHealthListener</listener-class>	
</listener>

<filter>
	<filter-name>HealthFilter</filter-name>
	<filter-class>com.eng.dotcms.healthchecker.filter.HealthFilter</filter-class>
</filter>

<filter-mapping>
	<filter-name>HealthFilter</filter-name>
	<url-pattern>/*</url-pattern>
</filter-mapping>