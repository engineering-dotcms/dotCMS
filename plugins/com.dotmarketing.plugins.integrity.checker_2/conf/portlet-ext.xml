
<!-- BEGIN: PORTLET DATA INTEGRITY -->	
<portlet>
	<portlet-name>EXT_INTEGRITY_CHECK_TOOL</portlet-name>
	<display-name>Data Integrity Check Tool</display-name>
	<portlet-class>com.liferay.portlet.JSPPortlet</portlet-class>
	<init-param>
			<name>view-jsp</name>
			<value>/plugins/com.dotmarketing.plugins.integrity.checker/view_diff_tool.jsp</value>
	</init-param>
	<init-param>
			<name>useWEBINFDIR</name>
			<value>false</value>
	</init-param>
	<expiration-cache>0</expiration-cache>
	<supports>
		<mime-type>text/html</mime-type>
	</supports>
	<resource-bundle>com.liferay.portlet.StrutsResourceBundle</resource-bundle>
	<security-role-ref>
		<role-name>CMS Administrator</role-name>
	</security-role-ref>
</portlet>	
<!-- END: PORTLET DATA INTEGRITY -->
